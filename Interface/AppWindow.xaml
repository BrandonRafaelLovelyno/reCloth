<Window x:Class="Interface.AppWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
        Title="AppWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Grid Background="#BD6D68">
                <StackPanel DockPanel.Dock="Left" Orientation="Vertical" VerticalAlignment="Center">
                    <Label Cursor="Hand" MouseLeftButtonDown="Marketplace_Click" Foreground="#FDF4EB" Content="&#xf290;" FontFamily="./#Font Awesome 6 Free Solid 6 Free Solid" FontSize="40" Margin="15"/>
                    <Label Cursor="Hand" MouseLeftButtonDown="Form_Click" Foreground="#FDF4EB" Content="&#xf093;" FontFamily="./#Font Awesome 6 Free Solid 6 Free Solid" FontSize="40" Margin="15"/>
                    <Label Cursor="Hand" MouseLeftButtonDown="Dashboard_Click" Foreground="#FDF4EB" Content="&#xf007;" FontFamily="./#Font Awesome 6 Free Solid 6 Free Solid" FontSize="40" Margin="15"/>
                </StackPanel>
            </Grid>
            <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
        </DockPanel>
    </Grid>
</Window>
